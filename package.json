{
  "name": "n8n-nodes-deep-ocr",
  "version": "0.1.0",
  "description": "n8n community node for Deep-OCR document processing API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "ocr",
    "deep-ocr",
    "document-processing",
    "text-extraction",
    "structured-data"
  ],
  "license": "MIT",
  "homepage": "https://github.com/h-teske/deep-ocr-n8n",
  "author": {
    "name": "Deep-OCR n8n Node Contributors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h-teske/deep-ocr-n8n.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DeepOcrApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DeepOcr/DeepOcr.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  }
}
